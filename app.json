{
  "expo": {
    "name": "newrctnv",
    "slug": "newrctnv",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/rn-icon.png",
    "splash": {
      "image": "./assets/rn-splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "self-blog-rn-dapp",
      "buildNumber": "1.0"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "package": "com.vankevych.self_blog_rn_app",
      "versionCode": 1,
      "permissions": ["CAMERA","WRITE_EXTERNAL_STORAGE"]
    }
  }
}
